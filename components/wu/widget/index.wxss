.global-widget{
  position: fixed;
  right: 0px;
  bottom: 0px;
  z-index: 999;
  display: inline-block;
  font-size: 28rpx;
  width: 30px;
  height: 30px;
  border-radius: 100px;
  overflow: hidden;
  padding: 5px;
  text-align: center;
  line-height: 28px;
  border: 1rpx solid #cbcbcb;
  background: -webkit-radial-gradient(#bcbcbc 20%, #fbfbfb 100%);
  /*box-shadow: 0px 6px 20px rgba(0, 0, 0, 0.2);*/
}
.global-widget image{
  width: 25px;
  height: auto;
}